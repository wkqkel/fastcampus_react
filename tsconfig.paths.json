{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@shared/*": [
        "src/components/shared/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@remote/*": [
        "src/remote/*"
      ],
      "@constants": [
        "src/constants/index"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@utils": [
        "src/utils/*"
      ]
    }
  }
}